{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build:dev": "vite build --mode development",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "audit:force": "npm audit fix --force",
    "security:check": "npm audit && echo 'Security audit complete'",
    "deploy:prepare": "npm run build && cd python_api && powershell -ExecutionPolicy Bypass -File scripts/package-for-godaddy.ps1",
    "deploy:frontend": "npm run build && echo 'Frontend built successfully. Upload dist/ contents to public_html/'",
    "deploy:backend": "cd python_api && powershell -ExecutionPolicy Bypass -File scripts/package-for-godaddy.ps1 && echo 'Backend packaged successfully. Upload python_api/app.zip to ~/api/'"
  },
